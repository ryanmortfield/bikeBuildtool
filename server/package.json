{
  "name": "bike-build-server",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "db:create": "wrangler d1 create bike-build-db",
    "db:migrate": "wrangler d1 migrations apply bike-build-db --remote",
    "db:migrate:local": "wrangler d1 migrations apply bike-build-db --local",
    "db:generate": "drizzle-kit generate",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@elysiajs/openapi": "^1.4.14",
    "drizzle-orm": "^0.36.0",
    "elysia": "^1.4.0"
  },
  "devDependencies": {
    "drizzle-kit": "^0.28.0",
    "vitest": "^2.1.0",
    "wrangler": "^4.65.0"
  }
}
